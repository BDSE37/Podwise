      "duration": "",
      "published_date": "",
      "apple_rating": 0,
      "chunk_text": "一開始讓我非常匪夷所思的一段話",
      "embedding": [],
      "language": "zh",
      "created_at": "2025-07-10T14:51:11.041569",
      "source_model": "text-embedding-ada-002",
      "tags": "[]"
