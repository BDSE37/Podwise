      "created_at": "2025-07-10T14:51:12.064079",
      "source_model": "text-embedding-ada-002",
      "tags": "[]"
    },
    {
      "chunk_id": "b40e6e36-6f95-48e5-a41c-45b76b1133c3",
      "chunk_index": 16,
      "episode_id": "6860f7421a30c174287a389e",
      "podcast_id": "1304",
