      "tags": "[\"感覺\", \"希望\", \"意識\"]"
    },
    {
      "chunk_id": "696dbbbf-ef64-4dce-bdc0-dd9519ba25d4",
      "chunk_index": 8,
      "episode_id": "6860f7421a30c174287a389e",
