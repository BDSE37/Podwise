      "tags": "[\"時事\"]"
    },
    {
      "chunk_id": "cbe9b735-c1f9-4c3a-9d39-7c9a747b2940",
      "chunk_index": 12,
      "episode_id": "6863806f13e684d30303f6c8",
      "podcast_id": "1321",
      "podcast_name": "",
