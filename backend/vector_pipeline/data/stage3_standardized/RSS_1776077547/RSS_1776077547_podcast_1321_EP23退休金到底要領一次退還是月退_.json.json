      "tags": "[\"投資\"]"
    },
    {
      "chunk_id": "da900943-8e1f-4963-8052-9866c217a4f8",
      "chunk_index": 31,
      "episode_id": "6863806f13e684d30303f6c8",
      "podcast_id": "1321",
      "podcast_name": "",
