      "duration": "",
      "published_date": "",
      "apple_rating": 0,
      "chunk_text": "Purple means the chewy tapioca balls",
      "embedding": [],
      "language": "zh",
      "created_at": "2025-07-10T14:45:02.679295",
      "source_model": "text-embedding-ada-002",
