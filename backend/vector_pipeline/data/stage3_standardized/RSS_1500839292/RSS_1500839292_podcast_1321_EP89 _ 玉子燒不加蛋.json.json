      "language": "zh",
      "created_at": "2025-07-10T14:53:28.258974",
      "source_model": "text-embedding-ada-002",
      "tags": "[\"行政法律\", \"投資\", \"刑事法律\"]"
    },
    {
      "chunk_id": "05537975-380a-44f6-90a9-de03bbe4f0b1",
      "chunk_index": 6,
      "episode_id": "6864df05b8a9f5c81761b5ce",
