      "published_date": "",
      "apple_rating": 0,
      "chunk_text": "Adam says the pilots have integrity, a noun meaning to have honesty and moral principles.",
      "embedding": [],
      "language": "zh",
      "created_at": "2025-07-10T14:45:07.801288",
      "source_model": "text-embedding-ada-002",
