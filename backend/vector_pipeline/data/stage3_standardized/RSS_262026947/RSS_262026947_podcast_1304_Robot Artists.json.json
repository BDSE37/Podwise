      "duration": "",
      "published_date": "",
      "apple_rating": 0,
      "chunk_text": "Hello, this is 6 Minute English from BBC Learning English. I'm Neil. And I'm Sam.",
      "embedding": [],
      "language": "zh",
      "created_at": "2025-07-10T14:45:13.365406",
      "source_model": "text-embedding-ada-002",
      "tags": "[]"
