      "duration": "",
      "published_date": "",
      "apple_rating": 0,
      "chunk_text": "OK, Neil, I'll reveal the correct answer later in the programme.",
      "embedding": [],
      "language": "zh",
      "created_at": "2025-07-10T14:45:06.527325",
      "source_model": "text-embedding-ada-002",
