      "tags": "[\"教育\", \"投資\"]"
    },
    {
      "chunk_id": "208beb80-3b1e-4b73-a885-7e1f60e31356",
      "chunk_index": 52,
      "episode_id": "6867815154dac0f7fe975865",
      "podcast_id": "1304",
      "podcast_name": "",
      "author": "",
