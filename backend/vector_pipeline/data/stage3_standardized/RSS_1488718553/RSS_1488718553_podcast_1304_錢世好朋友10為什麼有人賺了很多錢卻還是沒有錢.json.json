      "tags": "[\"投資\"]"
    },
    {
      "chunk_id": "4372ef19-3a78-4d84-8742-45439d2de2b1",
      "chunk_index": 43,
      "episode_id": "6867815154dac0f7fe975865",
      "podcast_id": "1304",
