      "tags": "[\"潛意識\", \"教育\", \"家庭\", \"意識\"]"
    },
    {
      "chunk_id": "b5fafd8d-4ccb-4efe-8996-81f61510594e",
      "chunk_index": 24,
      "episode_id": "6867815154dac0f7fe975826",
