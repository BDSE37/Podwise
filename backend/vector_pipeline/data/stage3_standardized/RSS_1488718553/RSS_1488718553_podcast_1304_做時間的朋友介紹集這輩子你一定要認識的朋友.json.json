      "tags": "[\"投資\", \"家庭\"]"
    },
    {
      "chunk_id": "feee7066-1ec4-4c7a-a37c-00d398dbad00",
      "chunk_index": 27,
      "episode_id": "6867815154dac0f7fe975736",
      "podcast_id": "1304",
      "podcast_name": "",
      "author": "",
