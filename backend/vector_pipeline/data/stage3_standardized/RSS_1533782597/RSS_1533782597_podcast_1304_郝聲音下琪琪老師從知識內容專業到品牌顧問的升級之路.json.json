      "duration": "",
      "published_date": "",
      "apple_rating": 0,
      "chunk_text": "我是誰我是什麼什麼公司的人",
      "embedding": [],
      "language": "zh",
      "created_at": "2025-07-10T14:45:29.832174",
      "source_model": "text-embedding-ada-002",
