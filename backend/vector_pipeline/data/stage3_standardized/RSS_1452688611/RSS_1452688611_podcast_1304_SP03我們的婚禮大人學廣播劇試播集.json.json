      "created_at": "2025-07-10T14:59:35.349045",
      "source_model": "text-embedding-ada-002",
      "tags": "[]"
    },
    {
      "chunk_id": "a7ea9d35-b7e4-4b3c-aa6c-545c83340514",
      "chunk_index": 35,
      "episode_id": "68637fe11508a79a775126a4",
