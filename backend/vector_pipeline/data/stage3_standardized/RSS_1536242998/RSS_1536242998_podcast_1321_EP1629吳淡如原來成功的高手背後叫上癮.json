      "tags": "[\"匯率\", \"投資\", \"健康\"]"
    },
    {
      "chunk_id": "5bf78dcb-450e-4ef6-8382-cd5bcfdf764f",
      "chunk_index": 28,
      "episode_id": "6860d9ad1a552e2cedaa8a3c",
