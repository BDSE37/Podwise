      "tags": "[]"
    },
    {
      "chunk_id": "439f38a7-764f-42ff-a193-095151cb38a8",
      "chunk_index": 31,
      "episode_id": "6860d9ad1a552e2cedaa8b2a",
      "podcast_id": "1321",
