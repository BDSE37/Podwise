      "source_model": "text-embedding-ada-002",
      "tags": "[\"投資\"]"
    },
    {
      "chunk_id": "7e6d1122-ef2e-41dc-a533-c475e2475ef0",
      "chunk_index": 32,
      "episode_id": "6860d9ad1a552e2cedaa8af6",
      "podcast_id": "1321",
      "podcast_name": "",
      "author": "",
