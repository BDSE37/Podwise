      "tags": "[\"特質\"]"
    },
    {
      "chunk_id": "1ea7beaf-855c-48f7-9fc0-25150f2b9589",
      "chunk_index": 23,
      "episode_id": "6860d9ad1a552e2cedaa89e6",
