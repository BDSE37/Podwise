      "language": "zh",
      "created_at": "2025-07-10T14:48:39.972695",
      "source_model": "text-embedding-ada-002",
      "tags": "[\"時事\"]"
    },
    {
      "chunk_id": "540237ba-ca57-405f-ab8e-e5ef54f6b35a",
      "chunk_index": 30,
      "episode_id": "6860d9ad1a552e2cedaa8b75",
      "podcast_id": "1321",
