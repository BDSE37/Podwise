      "tags": "[\"權利保障\", \"刑事法律\"]"
    },
    {
      "chunk_id": "aad7b3c4-141d-4ac7-b754-f3f185df2043",
      "chunk_index": 45,
      "episode_id": "6860d9ad1a552e2cedaa8a77",
