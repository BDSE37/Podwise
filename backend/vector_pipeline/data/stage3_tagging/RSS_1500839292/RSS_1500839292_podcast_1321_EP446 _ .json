      "tagged_at": "2025-07-10T14:52:42.556439"
    },
    {
      "chunk_text": "好咖代表古海小水手",
      "chunk_id": "68300868-40a1-44eb-a225-539c9c2198ec",
      "chunk_index": 64,
      "episode_id": "6864df05b8a9f5c81761b53f",
